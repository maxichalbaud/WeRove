<div class="aboutcontainer mx-3">
  <div class="header">
    <div class="container mx-0 px-0">
      <div class="title mt-2">
        <h1 class="pr-2">Sneak Peek</h1>
        <img src="data:image/png;base64," class="mt-1" id="flag">
      </div>
      <div class="location">
        <i class="fas fa-map-marker-alt fa-xs pr-1"></i><p>Local in <strong>Tortuga</strong></p>
      </div>
    </div>
      <img src="data:image/jpeg;base64," alt="" class="avatar-bordered my-2" id="avatar">
  </div>
  <p style="padding-top: 8px;"><%= 'Limited selection according to your interests. Login to experience more!' %></p>
</div>

<ul class="nav nav-tabs mx-3" id="myTab" role="tablist">
  <li class="nav-item">
    <a class="nav-link active" id="recommendations-tab" data-toggle="tab" href="#recommendations" role="tab" aria-controls="recommendations" aria-selected="false"><strong>Picks for you!</strong></a>
  </li>
</ul>
<div>
  <div class="about">
    <% @recommendations.each do |recommendation| %>
    <h2 class="category"><%= recommendation.category %></h2>
      <div>
        <div class="scrolling-wrapper">
          <%= render 'recomcard', recommendation: recommendation %>
        </div>
      </div>
    <% end %>
  </div>
</div>
