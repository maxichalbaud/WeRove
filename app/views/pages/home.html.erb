<% if user_signed_in? %>

<div class="home-container">
  <% if !user_signed_in? %>
  <h1 class="title">Join the greatest traveler community.</h1>
  <h2 class="subtitle">Find a local friend everywhere.</h2>
  <% else %>
  <h1 class="title"><%= current_user.first_name.nil? ? "Welcome again!" : "Hi, #{current_user.first_name}!" %></h1>
  <h2 class="subtitle">What do you want to do today?</h2>
  <% end %>

  <form id="home-form" action="matches" method="POST">
    <% if !user_signed_in? %>
  <label class="label" style="margin-bottom: 0px;"> For more accurate results, tell us which word fits you better:</label>
    <%= render 'layouts/components/tags' %>
    <% end %>
    <div class="Where">
      <label class="label" for="locationInput">Where are you going?</label>
        <input type="text" class="form-control" id="location" placeholder="" name="location">
    </div>
    <div class="When">
      <label class="label" for="DateInput">When are you going?</label>
        <input type="text" class="form-control" id="datepicker" placeholder="Select your dates" name="date">
    </div>
    <% if user_signed_in? %>
    <div class="">
      <input type="submit" value="Find a local" class="btn btn-primary" id="findLocals">
    </div>
  </form>
  <input type="submit" value="Recommend me" class="btn btn-outline-primary" id="Recommend" name="user_recommendations">
   <% else %>
   <input type="submit" value="Find a local" class="btn btn-primary" id="findLocals" disabled>
  <input type="submit" value="Recommend me" class="btn btn-primary" id="Recommend" name="recommendations">
   <% end %>
</div>
<%= render 'loader' %>

<% else %>

<div id="carouselExampleIndicators" class="carousel slide home-slides" data-ride="carousel">
  <ol class="carousel-indicators">
    <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
    <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
    <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
    <li data-target="#carouselExampleIndicators" data-slide-to="3"></li>
  </ol>
  <div class="carousel-inner">
    <div class="carousel-item active h-100" style="padding-bottom: 130px;">
      <div class="d-flex flex-column h-100 justify-content-around">
        <div class="h-100">
          <h3 style="font-family: 'Minute'; color: white; font-size: 120px; padding-left: 12px; padding-top: 12px; text-shadow: 3px 3px #747fda; text-align: center; margin-top: 36px; justify-content: center;">wR</h3>
          <h1 style="text-align: center; font-size: 34px; margin-top: 30px; margin-bottom: 30px;">Join the greatest travelers community</h1>
        </div>
        <div class="">
          <%= link_to "Sign Up", new_user_registration_path, class: "btn btn-light btn-slides w-100" %>
          <%= link_to "I'm already a user", new_user_session_path, class: "btn btn-outline-primary landing-btn", style: "margin-top: 20px;" %>
        </div>
      </div>
    </div>
